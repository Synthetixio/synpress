{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "types/**"]
    },
    "test": {
      "dependsOn": ["build"]
    },
    "build:cache": {
      "dependsOn": ["build"]
    },
    "test:e2e:headful": {
      "dependsOn": ["build", "build:cache"]
    },
    "docs:build": {
      "outputs": ["docs/.vitepress/dist"]
    }
  }
}
